// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Setting {
  name  String      @unique
  value String
}

model Host {
  id Int            @id @default(autoincrement())
  name String       @unique
  host String
  protocol String?
  port Int?
}